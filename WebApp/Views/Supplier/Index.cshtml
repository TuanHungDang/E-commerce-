@model IEnumerable<Supplier>
    <table class="table table-bordered">
         
             <tr class="table-dark">
            <th>ID</th>
            <th>Company Name</th>
            <th>Logo</th>
            <th>Contact Name</th>
            <th>Email</th>
            <th>Address</th>
            <th>Phone</th>
            <th>Description</th>
            <th>Command</th>
        </tr>
        
      
         @foreach(var item in Model){
            <tr>
                <td>@item.SupplierId</td>
                <td>@item.CompanyName</td>
                <td>@item.Logo</td>
                <td>@item.ContactName</td>
                <td>@item.Email</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>
                <td>@item.Description</td>
                <td> <a class="btn btn-outline-primary" href="/Supplier/details/@item.SupplierId">Details</a></td>
            </tr>
        }

      
       
    </table>